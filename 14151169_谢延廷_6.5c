#include <stdio.h>
#define h 5
int main ()
{

	int i,temp,a[h];
      printf("输入一串含有五个数字的数\n");
	  for (i=0;i<h;i++)
		  scanf("%d",&a[i]);
	  for (i=0;i<h/2;i++)
	  {temp=a[i];
	      a[i]=a[h-i-1];
		  a[h-i-1]=temp;}  
	  
	  
	  
	  for (i=0;i<h;i++)
		  printf("%4d",a[i]);
		  printf("\n");
		  return 0;
}
